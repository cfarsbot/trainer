AAA,EAAA,KAAA;AACA,aAAA,SAAA,GAAA,KAAA,OAAA,CAAA,CAAA,EAAA,SAAA;AACA,aAAA,OAAA,GAAA,KAAA,OAAA,CAAA,CAAA,EAAA,OAAA;AAEA;AAEA,KAbA;AAeA,IAAA,cAfA,4BAeA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,KAjBA;AAmBA,IAAA,YAnBA,0BAmBA;AACA,WAAA,gBAAA,GAAA,KAAA;AACA,KArBA;AAuBA,IAAA,OAvBA,qBAuBA;AAEA,WAAA,YAAA;AACA,WAAA,UAAA,CAAA,KAAA,WAAA;AACA,KA3BA;AA6BA,IAAA,iBA7BA,6BA6BA,OA7BA,EA6BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;;AAEA,UAAA,OAAA,IAAA,KAAA,OAAA,EAAA;AAAA;AACA,aAAA,OAAA,CAAA,MAAA,CAAA,KAAA,YAAA,EAAA,CAAA;AACA,aAAA,YAAA,GAAA,EAAA;AACA,aAAA,KAAA,GAAA,EAAA;AACA,aAAA,OAAA,GAAA,EAAA;AACA,aAAA,SAAA,GAAA,EAAA;AACA,aAAA,UAAA,GAAA,KAAA;AACA,aAAA,UAAA,CAAA,OAAA;AACA;AACA,KA1CA;AA4CA,IAAA,gBA5CA,4BA4CA,SA5CA,EA4CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AAEA,WAAA,KAAA,GAAA,KAAA,OAAA,CAAA,SAAA,EAAA,KAAA;AACA,WAAA,SAAA,GAAA,KAAA,OAAA,CAAA,SAAA,EAAA,SAAA;AACA,WAAA,OAAA,GAAA,KAAA,OAAA,CAAA,SAAA,EAAA,OAAA;AACA,KAlDA;AAoDA;AACA;AACA,IAAA,UAtDA,sBAsDA,IAtDA,EAsDA;AAAA;;AACA,UAAA,IAAA,GAAA,UAAA,CAAA,OAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,UAAA,IAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,WAAA,MAAA,CAAA,QAAA,gBAAA,MAAA,CAAA,QAAA,uBAAA;AACA,UAAA,OAAA,EAAA;AAAA,4BAAA;AAAA,WADA;AAEA,UAAA,IAAA,EAAA,IAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA;AACA,SAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAVA;AAWA;AACA,KAtEA;AAuEA;AACA;AACA,IAAA,UAzEA,sBAyEA,OAzEA,EAyEA;AAAA;;AACA,UAAA,IAAA,GAAA,UAAA,CAAA,OAAA,CAAA,IAAA;AACA,MAAA,KAAA,CAAA,MAAA,WAAA,MAAA,CAAA,QAAA,gBAAA,MAAA,CAAA,QAAA,gBAAA;AACA,QAAA,IAAA,EAAA;AAAA,UAAA,OAAA,EAAA,OAAA;AAAA,UAAA,IAAA,EAAA;AAAA;AADA,OAAA,EAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OARA;AASA,KApFA;AAsFA,IAAA,OAtFA,qBAsFA;AAAA;;AACA,UAAA,IAAA,GAAA,UAAA,CAAA,OAAA,CAAA,IAAA;AACA,MAAA,KAAA,CAAA,IAAA,WAAA,MAAA,CAAA,QAAA,gBAAA,MAAA,CAAA,QAAA,gBAAA;AACA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA,SADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA;AAlGA;AA1BA,CAAA","sourcesContent":["<!--  Childs: Navigation  /components/navigation.vue\n              Table       /components/basic_list.vue\n              List        /components/curd_table.vue\n               \n      Displays all Wordlists of the (logged in) user\n      Create new Wordlists, edit existing ones\n      -->\n\n<template>\n  <div>\n    <Navigation />\n    <v-container fluid>\n      <v-row no-gutters>\n        <v-col md=\"2\">\n          <!--\n            Show all (word)lists\n            entrys:\"all lists to be shown\"\n               -->\n          <List class=\"mt-4\" :entrys=\"content\" @selected-item=\"SelectionHandler\"/>\n          <v-btn color=\"primary\" class=\"mt-2\" @click=\"newListDialoge\">Liste erstellen</v-btn>\n        </v-col>\n        <v-col md=\"10\">\n          <!-- \n            description: Show all words of selected list\n\n            scope=:       refers to @selected-item (List component)\n                          holds index value of the list that the user selects\n            \n            words=:       holds the words to be diplayed in an arraylist\n\n            list_id=:     id of selected list\n            list_name:    name of selected list\n\n            @delete-list: called, when button \"Delete list\" clicked\n            \n            @update:      called, when user creates new word\n\n            class:        margin left: 3, margin top: 4 (vuetify values, not px)\n             -->\n\n          <Table v-if=\"show_table\"\n          :scope=\"selectedItem\" \n          :words=\"words\" \n          :list_id=\"list_id\" \n          :list_name=\"list_name\" \n          @delete-list=\"deleteListHandler\"\n          @update=\"init\"\n          class=\"ml-3 mt-4\"/>\n          <div v-else>\n            Keine Wörterliste gefunden, erstelle doch eine!\n          </div>\n          </v-col>\n      </v-row>\n\n       <v-dialog v-model=\"dialogCreateList\" max-width=\"500px\">\n            <v-card>\n              <v-card-title class=\"headline\">Wie soll die neue Liste heißen?</v-card-title>\n\n           <v-card-text>\n                <v-container>\n                  <v-row>\n                    <v-col\n                      cols=\"12\"\n                      sm=\"12\"\n                      md=\"12\"\n                    >\n                      <v-text-field\n                        v-model=\"newListName\"\n                        label=\"Name\"\n                      ></v-text-field>\n                    </v-col>\n            \n                  </v-row>\n                </v-container>\n              </v-card-text>\n\n\n              \n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn color=\"blue darken-1\" text @click=\"newList\">Speichern</v-btn>\n                <v-btn color=\"blue darken-1\" text @click=\"closeDialoge\">Abbrechen</v-btn>                \n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n  \n    </v-container>\n    \n    \n    \n  \n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport config from '../../config'\n\nimport Navigation from '../components/navigation.vue'\nimport List from '../components/basic_list.vue'\nimport Table from '../components/curd_table.vue'\n\nimport user_store from '../store/user'\n\nexport default {\n  name: 'Home',\n\n  data(){\n        return{\n            selectedItem: \"\",\n            content:[],\n            words: [],\n            list_id: \"\",\n            list_name: \"\",\n            show_table: false,\n            dialogCreateList: false,\n            newListName: \"\"\n        }\n    },\n\n    components: {\n      List,\n      Table,\n      Navigation\n    }, \n    mounted(){\n      // get content\n      this.getList()\n    },\n  \n    methods: {\n      init(data){\n        this.content = data\n        if (typeof (data[0]).list_name != undefined){\n          console.log(\"there is data\")\n          console.log(data)\n          this.show_table = true\n          this.words = this.content[0].array\n          this.list_name = this.content[0].list_name\n          this.list_id = this.content[0].list_id\n          \n        }\n    \n      },\n\n      newListDialoge(){\n        this.dialogCreateList = true  \n      },\n\n      closeDialoge(){\n        this.dialogCreateList = false\n      },\n\n      newList(){\n        \n        this.closeDialoge()\n        this.createList(this.newListName)\n      },\n\n      deleteListHandler(list_id){\n        console.log(list_id)\n        console.log(this.content)\n\n        if(list_id == this.list_id){ // should be allways the case, but you never know\n          this.content.splice(this.selectedItem, 1)\n          this.selectedItem = \"\"\n          this.words = \"\"\n          this.list_id = \"\"\n          this.list_name = \"\"\n          this.show_table = false\n          this.deleteList(list_id)\n        }\n      },\n\n      SelectionHandler(selection){\n        console.log(selection)\n\n        this.words = this.content[selection].array\n        this.list_name = this.content[selection].list_name\n        this.list_id = this.content[selection].list_id\n        },\n\n      // Create new list\n      // data: Name of new list\n      createList(data){\n        var user = user_store.getters.user\n        console.log(data)\n        if(data != \"\"){\n          axios.put(`${config.protocol}://${config.hostname}/create_wordlist`, {\n            headers: {'Content-Type': 'application/json'},\n            data,\n            user,\n            })\n            .then( response => {\n              this.init(response.data)\n            })\n            .catch(function (error) {\n              console.log(error)\n            });\n          }     \n      },\n      // deletes list\n      // list_id: id of the list about to be deleted\n      deleteList(list_id){\n        var user = user_store.getters.user\n          axios.delete(`${config.protocol}://${config.hostname}/wordlist`, {                        \n            data:{ list_id, user}\n            })\n            .then( response => {\n              this.init(response.data)                            \n            })\n            .catch(function (error) {\n              console.log(error)\n            });\n      },\n\n      getList(){\n        var user = user_store.getters.user\n        axios.post(`${config.protocol}://${config.hostname}/wordlist`, {\n          headers: {'Content-Type': 'application/json'},\n          user\n          })\n          .then( response => {\n            this.init(response.data)                            \n          })\n          .catch(function (error) {\n            console.log(error)\n          });\n      },\n    }\n  }\n\n\n</script>\n"],"sourceRoot":"src/views"}]}