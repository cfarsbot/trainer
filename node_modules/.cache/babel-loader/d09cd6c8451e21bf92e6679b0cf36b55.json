AoBA;AAAA;;AACA,UAAA,IAAA,GAAA,UAAA,CAAA,OAAA,CAAA,IAAA;AACA,MAAA,KAAA,CAAA,IAAA,WAAA,MAAA,CAAA,QAAA,gBAAA,MAAA,CAAA,QAAA,gBAAA;AACA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA,SADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA,KAhCA;AAiCA,IAAA,WAjCA,uBAiCA,OAjCA,EAiCA,aAjCA,EAiCA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,WAAA,MAAA,CAAA,QAAA,gBAAA,MAAA,CAAA,QAAA,qBAAA;AACA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA,SADA;AAEA,QAAA,IAAA,EAAA;AACA,UAAA,EAAA,EAAA;AADA,SAFA;AAKA,QAAA,IAAA,EAAA;AACA,UAAA,OAAA,EAAA,OADA;AAEA,UAAA,aAAA,EAAA;AAFA;AALA,OAAA,EAYA,IAZA,CAYA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAlBA;AAmBA;AArDA,GAhBA;AAwEA,EAAA,OAxEA,qBAwEA;AACA,SAAA,OAAA;AACA,GA1EA;AA2EA,EAAA,UAAA,EAAA;AACA,IAAA,IAAA,EAAA,IADA;AAEA,IAAA,IAAA,EAAA,IAFA;AAGA,IAAA,UAAA,EAAA,UAHA;AAIA,IAAA,IAAA,EAAA;AAJA;AA3EA,CAAA","sourcesContent":["<template>\n    <div>\n      <Navigation />\n    <v-container fluid>\n      <v-row no-gutters>\n        <v-col md=\"2\">\n          Welche Sprache willst du dich Testen?\n        \n        \n            <!-- Wordlists -->\n            <Menu class=\"mt-4\" :entrys=\"supported_lang\" @selected-item=\"SelectionLangHandler\"/>\n            <List class=\"mt-4\" :entrys=\"content\" @selected-item=\"SelectionListHandler\"/>\n             \n            \n          <v-btn\n          class=\"mt-4\"\n          @click=\"submit\"\n          color=\"primary\"\n          dark\n          >Start</v-btn>\n        </v-col>\n        <v-col md=\"10\">\n          <Play :words=\"play_list\" :list_id=\"list_id\" :lang=\"selected_lang\" :showResults=\"showResults\"/>\n        </v-col>\n      </v-row>\n    </v-container>\n    </div>\n\n</template>\n\n<script>\nimport axios from 'axios'\nimport config from '../../config'\nimport user_store from '../store/user'\n\nimport Navigation from '../components/navigation'\nimport Menu from '../components/basic_menu'\n\nimport List from '../components/basic_list'\nimport Play from '../components/play_words'\n//import user_store from '../store/user'\n export default {\n  name: 'Login',\n      \n  data (){\n    return{\n      content: [],\n      play_list: [],\n      play_lang: \"\",\n      list_name: \"\",\n      list_id: \"\",\n      supported_lang: ['Deutsch', 'Englisch'],\n      selected_lang: \"\",\n      showResults: false\n      }\n  },\n\n  methods: {\n\n    submit(){\n      if(this.list_id != \"\" || this.selected_lang != \"\"){\n        this.getPlaylist(this.list_id, this.selected_lang)\n       \n      }\n\n      \n    },\n    SelectionLangHandler(selection){\n      this.selected_lang = selection\n      this.submit()\n    },\n    SelectionListHandler(selection){\n      \n      console.log(selection)\n        this.list_name = this.content[selection].list_name\n        this.list_id = this.content[selection].list_id\n    },\n    getList(){\n      var user = user_store.getters.user\n        axios.post(`${config.protocol}://${config.hostname}/wordlist`, {\n          headers: {'Content-Type': 'application/json'},\n            user\n        })\n        .then( response => {\n          this.content = response.data\n        })\n        .catch(function (error) {\n          console.log(error)\n        });\n    },\n    getPlaylist(list_id, selected_lang){\n      axios.post(`${config.protocol}://${config.hostname}/play_wordlist`, {\n        headers: {'Content-Type': 'application/json'},\n          user:{\n            id: 7\n          },\n          data:{\n            list_id,\n            selected_lang\n          }\n\n          \n        })\n        .then( response => {\n          console.log(response.data)\n          this.play_list = response.data\n        })\n        .catch(function (error) {\n          console.log(error)\n        });\n    }\n        \n  },\n  mounted(){\n    this.getList()\n  },\n  components:{\n    List,\n    Play,\n    Navigation,\n    Menu\n  } \n }\n\n</script>\n"],"sourceRoot":"src/views"}]}